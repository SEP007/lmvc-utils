{
    "name": "scandio/lmvc-utils",
    "type": "library",
    "description": "LMVC-utils is a set of utilities for lmvc, lmvc-modules and troba",
    "keywords": ["php", "library", "lmvc", "scandio", "lmvc-modules", "troba"],
    "homepage": "https://github.com/scandio/lmvc-utils",
    "license": "MIT",
    "minimum-stability": "stable",

    "authors": [
        {
            "name": "Scandio GmbH",
            "homepage": "http://www.scandio.de/"
        }
    ],

    "require": {
        "php": ">=5.4"
    },
    "repositories": [

    ],
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "satooshi/php-coveralls": "dev-master"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": {
            "Scandio\\lmvc\\utils": "lib/"
        }
    }
}
